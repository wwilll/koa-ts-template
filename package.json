{
  "name": "koa-ts-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development tsx watch index.ts",
    "debug": "cross-env NODE_ENV=development tsx --inspect --watch index.ts",
    "build": "rm -rf dist/* && tsc --project ./ && bash ./filemove.sh",
    "docker_nocache": "docker build --no-cache -t koa-ts-template:1.0.0 .",
    "docker": "docker build -t koa-ts-template:1.0.0 .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "bash -c \"if [ -d .husky ] && [ ! -d .husky/_ ]; then husky && echo husky init; fi\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/router": "^14.0.0",
    "koa": "^3.0.1",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@types/koa": "^3.0.0",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^24.3.0",
    "cross-env": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,md}": [
      "prettier --write"
    ]
  }
